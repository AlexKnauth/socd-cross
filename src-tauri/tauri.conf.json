{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": [
      "deb",
      "msi",
      "appimage"
    ],
    "windows": {
      "allowDowngrades": true,
      "certificateThumbprint": null,
      "digestAlgorithm": null,
      "nsis": null,
      "timestampUrl": null,
      "tsp": false,
      "wix": null,
      "webviewInstallMode": {
        "silent": true,
        "type": "downloadBootstrapper"
      }
    },
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "createUpdaterArtifacts": "v1Compatible"
  },
  "productName": "OverBind",
  "mainBinaryName": "OverBind",
  "version": "1.4.2",
  "identifier": "com.github.cjonas1999.overbind",
  "plugins": {
    "updater": {
      "pubkey": "",
      "windows": {
        "installMode": "passive",
        "installerArgs": []
      }
    }
  },
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "OverBind",
        "width": 800,
        "height": 600,
        "label": "main",
        "url": "index.html",
        "focus": true
      }
    ],
    "security": {
      "csp": null
    },
    "trayIcon": {
      "iconPath": "icons/32x32.png"
    }
  }
}